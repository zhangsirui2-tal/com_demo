{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "stripInternal": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "declaration": true,
    "baseUrl": ".",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@zsrcom/components": ["packages/components/src"],
      "@zsrcom/example-com": ["packages/example-com/src"],
      "@zsrcom/antd-v5": ["packages/antd-v5/src"],
    }
  },
  "include": [".dumirc.ts", "packages/**/*", "src/**/*"],
  "exclude": ["node_modules", "dist", "docs-dist"]
}
